{
  "name": "yuu-lib",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/yuu-lib.umd.js",
  "module": "./dist/yuu-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/yuu-lib.es.js",
      "require": "./dist/yuu-lib.umd.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.63.6",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vue-tsc": "^1.8.3"
  }
}
